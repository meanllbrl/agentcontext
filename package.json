{
  "name": "agentcontext",
  "version": "0.1.0",
  "description": "AI agent persistent context management CLI",
  "type": "module",
  "bin": {
    "agentcontext": "./dist/index.js"
  },
  "files": [
    "dist",
    "skill",
    "agents",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:dashboard": "cd dashboard && npm run build",
    "build:cli": "tsup",
    "build": "npm run build:dashboard && npm run build:cli",
    "dev": "tsup --watch",
    "dev:dashboard": "cd dashboard && npm run dev",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agent",
    "context",
    "memory",
    "cli",
    "claude",
    "gemini",
    "llm"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.10.1",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "nanoid": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "sharp": "^0.34.5",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.6"
  }
}
